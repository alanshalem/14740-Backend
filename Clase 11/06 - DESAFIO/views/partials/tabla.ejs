<section class="container pt-5">
  <h1>Historial</h1>
  <% if (usuarios.length === 0) { %>
  <p>No se encontraron datos</p>
  <% } else { %>
  <table class="table table-striped table-dark">
    <thead>
      <tr>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>Edad</th>
      </tr>
    </thead>
    <tbody>
      <% for (const usuario of usuarios) { %>
      <tr>
        <td><%= usuario.nombre %></td>
        <td><%= usuario.apellido %></td>
        <td><%= usuario.edad %></td>
      </tr>
      <% } %>
    </tbody>
  </table>
  <% } %>
</section>
